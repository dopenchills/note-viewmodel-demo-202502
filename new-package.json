{
  "private": true,
  "name": "note-vm-demo",
  "version": "1.0.0",
  "description": "Note VM Demo with AI Voice and Vue.js Web UI",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "format": "prettier --write \"**/*.{js,ts,json,vue,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,vue,css,md}\"",
    "lint": "eslint . --ext .ts,.vue",
    "lint:fix": "eslint . --ext .ts,.vue --fix",
    "dev:web.vue": "vite --config vite.config.web.vue.ts",
    "dev:ai.voice": "vite --config vite.config.ai.voice.ts",
    "build:web.vue": "vue-tsc --noEmit && vite build --config vite.config.web.vue.ts",
    "build:ai.voice": "tsc --noEmit && vite build --config vite.config.ai.voice.ts",
    "preview:web.vue": "vite preview --config vite.config.web.vue.ts",
    "preview:ai.voice": "vite preview --config vite.config.ai.voice.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "inversify": "^6.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.4",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.21.0",
    "eslint-plugin-vue": "^9.32.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.2",
    "typescript": "~5.7.2",
    "vite": "^6.1.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.5",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.2"
  }
}
